			astroToday = "������";
			astroArr = 1;
		}else if(nowTimeMonth == 5 && nowTimeDay >= 21 || nowTimeMonth == 6 && nowTimeDay <= 21){
			astroToday = "������";
			astroArr = 2;
		}else if(nowTimeMonth == 6 && nowTimeDay >= 22 || nowTimeMonth == 7 && nowTimeDay <= 22){
			astroToday = "������";
