					dyLeftDataTempArr.push('" target="_blank" href="'+obj.chs.dy.data[i].items[n].url+'">'+obj.chs.dy.data[i].items[n].title+'</a>');
					if(obj.chs.dy.data[i].items[n].ext){ //����
						if(obj.chs.dy.data[i].items[n].ext.split == "1"){ //����
							dyLeftDataTempArr.push(' | ');
						}else{
							dyLeftDataTempArr.push(' ');
						}
					}else{
						dyLeftDataTempArr.push(' ');
					}
				}
				dyLeftDataTempArr.push('</li>');
			}else{
				dyLeftDataTempArr.push('<li><a class="');
				if(obj.chs.dy.data[i].ext){
					if(obj.chs.dy.data[i].ext.style.bold == "1"){ //����
						dyLeftDataTempArr.push('Q-bold ');
